<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dbnav" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="NativeStorage">
        <param name="android-package" value="NativeStorage" />
    </feature>
    <feature name="videoOverlay">
        <param name="android-package" value="com.ezartech.ezar.videooverlay.VideoOverlayPlugin" />
        <param name="onload" value="false" />
    </feature>
    <feature name="CameraPreview">
        <param name="android-package" value="com.cordovaplugincamerapreview.CameraPreview" />
        <param name="onload" value="true" />
    </feature>
    <name>鵬程灣里</name>
    <description>
       大鵬灣地區相關景點介紹導覽App
    </description>
    <author email="motea927@yahoo.com.tw" href="https://codepen.io/motea927/">
        李學澤Mt(motea927)
    </author>
    <content src="index.html" />
    <access origin="https://1031241139.000webhostapp.com/*" />
    <access origin="http://*.google.com" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
